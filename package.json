{
  "name": "enonic-wizardry",
  "version": "0.0.5",
  "description": "Functional utility library for Enonic XP",
  "main": "lib/index.js",
  "module": "es6/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib/* es6/*",
    "build": "npm run clean && tsc && tsc -p tsconfig.es6.json && npm run build:cli",
    "build:cli": "tsc -outDir bin cli/xml-to-ts.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ItemConsulting/enonic-wizardry.git"
  },
  "keywords": [
    "enonic",
    "fp"
  ],
  "author": "Tom Arild Jakobsen <tajakobsen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ItemConsulting/enonic-wizardry/issues"
  },
  "homepage": "https://github.com/ItemConsulting/enonic-wizardry#readme",
  "dependencies": {
    "@types/node": "^12.7.4",
    "enonic-fp": "^0.0.10",
    "fp-ts": "^2.0.5",
    "xmldom": "^0.1.27",
    "xpath": "0.0.27"
  },
  "devDependencies": {
    "rimraf": "^3.0.0",
    "typescript": "^3.5.3"
  }
}
